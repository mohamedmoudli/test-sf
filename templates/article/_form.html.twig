{{ form_start(form, {'attr': {'novalidate': 'novalidate','id': 'formId'}}) }}

<div class="form-group">
    <label class="main-content-label tx-11 tx-medium tx-gray-600">Title</label>
    {{ form_widget(form.title, {'attr': {'class': 'form-control'}}) }}
</div>
<div class="form-group">
    <label class="main-content-label tx-11 tx-medium tx-gray-600">Date</label>
    {{ form_widget(form.date, {'attr': {'class': 'form-control'}}) }}
</div>
<div class="form-group">
    <label class="main-content-label tx-11 tx-medium tx-gray-600">Text</label>
    {{ form_widget(form.text, {'attr': {'class': 'form-control'}}) }}
</div>
<div class="form-group">
    <label class="main-content-label tx-11 tx-medium tx-gray-600">Photo</label>
    {{ form_widget(form.imageFile, {'attr': {'class': 'form-control'}}) }}
</div>


<button class="btn ripple btn-main-primary">{{ button_label|default('Save') |trans }}</button>
{{ form_end(form) }}
